{
	"manifest_version": 2,
	"name": "Repro",
	"version": "1.1",
	"browser_action": {
		"default_title": "Repro"
	},
	"content_scripts": [{
		"matches": ["*://*/*"],
		"js": ["./bundle.js"]
	}],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"storage",
		"management"
	]
}
