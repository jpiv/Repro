{
	"manifest_version": 2,
	"name": "Repro",
	"version": "1.3.0",
	"browser_action": {
		"default_title": "Repro",
		"default_popup": "popup/index.html"
	},
	"content_scripts": [{
		"matches": ["*://*/*"],
		"js": ["./app_bundle.js"]
	}],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"storage",
		"management"
	]
}
